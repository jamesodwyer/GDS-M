  "layout": {
    "spacing": {
      "inBetweenCards": {
        "$type": "dimension",
        "$value": "{core.dimension.600}",
        "$description": "This is the spacing between card elements i.e. carousels, event cards"
      },
      "inBetweenInteractive": {
        "$type": "dimension",
        "$value": "{core.dimension.400}",
        "$description": "This is the spacing between 2 or more interactive elements i.e. button, chips"
